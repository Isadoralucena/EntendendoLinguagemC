#include <stdio.h>
#include <stdlib.h>

int main(void){
    int matriz[3][3]={1,2,3,4,5,6,7,8,9};
    //Tem que especificar pelo menos a coluna(segundo colchete)
    void funcaoPrintf(int x[][3]);
    funcaoPrintf(matriz);//Chamando

    system("pause");
    return 0;
}
void funcaoPrintf(int x[][3]){
    int i,j;
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("%i",x[i][j]);
        }
        printf("\n");
    }

}
