#include <stdio.h>
#include <stdlib.h>

int main(void){
    int i;
    for(i=1;i<=20;i++){
        if((i%3==0)&&(i%9==0)){
            printf("%i eh divisivel por 3 e 9\n",i);
            break;//Break encerra o loop ao atingir o primeiro número divível por 3 e 9
        }else{
            printf("%i eh indivisivel por 3 e 9\n",i);
        }

    }
}
